prometheus:
  service:
    type: LoadBalancer
    loadBalancerIP: "{{ .StateValues.ip.kind2.prometheus }}"
  prometheusSpec:
    externalLabels:
      cluster: kind2
    remoteWrite:
      - url: http://{{ .StateValues.ip.kind1.victoriaMetrics }}:8428/api/v1/write

grafana:
  enabled: false
alertmanager:
  enabled: false
